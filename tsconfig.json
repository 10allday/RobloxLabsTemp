{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"jsx": "react",
		"outDir": "server",
		"rootDir": "src",
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"preserveSymlinks": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"stripInternal": true,
		"removeComments": true
	}
}
